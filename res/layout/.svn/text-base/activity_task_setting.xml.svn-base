<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:jsako="http://schemas.android.com/apk/res/com.jsako.mobilesafe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#8866ff00"
        android:gravity="center"
        android:text="进程管理设置"
        android:textColor="#000000"
        android:textSize="22sp" />

    <com.jsako.mobilesafe.ui.SettingItemView
        android:id="@+id/siv_showsystem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/textview_selector"
        jsako:desc_off="不显示"
        jsako:desc_on="显示"
        jsako:itemtitle="显示系统进程" />

    <com.jsako.mobilesafe.ui.SettingItemView
        android:id="@+id/siv_killprocess_ontime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/textview_selector"
        jsako:desc_off="未开启"
        jsako:desc_on="开启"
        jsako:itemtitle="锁屏清理进程" />

</LinearLayout>